\par             logger.info(f"Processed \{processed_entities\}/\{total_entities\} entities.")
\par 
\par         # Execute operations in batches
\par         if len(operations) >= batch_size:
\par             logger.info(f"Writing batch of \{len(operations)\} entities to the database.")
\par             try:
\par                 result = linked_entities_collection.bulk_write(operations, ordered=False)
\par                 logger.info(f"Bulk upserted \{result.upserted_count + result.modified_count\} linked entities.")
\par             except Exception as e:
\par                 logger.error(f"Error bulk upserting linked entities: \{e\}")
\par                 raise e
\par             operations = []
\par 
\par     # Execute remaining operations
\par     if operations:
\par         logger.info(f"Writing final batch of \{len(operations)\} entities to the database.")
