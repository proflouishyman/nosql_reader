\par def fuzzy_match(term, reference_terms, threshold=90):
\par     """
\par     Perform fuzzy matching to find the best match for a term in reference_terms.
\par     Returns the matched term if similarity exceeds the threshold, else None.
\par     """
\par     try:
\par         result = process.extractOne(term, reference_terms, scorer=fuzz.token_sort_ratio)
\par         if result is None:
\par             logger.debug(f"No fuzzy match found for term '\{term\}'.")
\par             return None
\par 
\par         match, score = result
\par         logger.debug(f"Fuzzy match for term '\{term\}': '\{match\}' with score \{score\}.")
\par 
\par         if score >= threshold:
\par             return match
\par         return None
\par     except Exception as e:
\par         logger.error(f"Exception during fuzzy matching for term '\{term\}': \{e\}")
\par         return None
\par 
\par def fetch_wikidata_entity(term):
\par     """
\par     Fetch Wikidata entity ID for a given term using the Wikidata API.
\par     Returns the entity ID if found, else None.
\par     """
\par     wikidata_cache = \{\}
\par     if term in wikidata_cache:
\par         return wikidata_cache[term]
\par     try:
\par         url = "https://www.wikidata.org/w/api.php"
\par         params = \{
\par             'action': 'wbsearchentities',
\par             'search': term,
\par             'language': 'en',
\par             'format': 'json'
\par         \}
\par         response = requests.get(url, params=params, timeout=5)
\par         data = response.json()
\par         if 'search' in data and len(data['search']) > 0:
\par             # Return the first matching entity ID
\par             wikidata_id = data['search'][0]['id']
\par             wikidata_cache[term] = wikidata_id
\par             logger.debug(f"Fetched Wikidata ID '\{wikidata_id\}' for term '\{term\}'.")
\par             return wikidata_id
\par         else:
\par             wikidata_cache[term] = None
\par             logger.debug(f"No Wikidata ID found for term '\{term\}'.")
\par             return None
\par     except Exception as e:
