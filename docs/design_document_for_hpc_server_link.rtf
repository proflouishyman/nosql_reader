\par def link_entities(aggregated_entities, linked_entities_collection, existing_linked_terms, link_wikidata, fuzzy_threshold, batch_size=1000):
\par     """
\par     Link aggregated entities and update the linked_entities collection.
\par     """
\par     linked_count = 0
\par     processed_entities = 0
\par     total_entities = len(aggregated_entities)
\par     logger.info(f"Total unique entities to process: \{total_entities\}")
\par 
\par     operations = []
\par 
\par     for (term_lower, ent_type), data in aggregated_entities.items():
\par         frequency = data['frequency']
\par         document_ids = list(data['document_ids'])
\par         wikidata_id = None
\par 
\par         if link_wikidata:
\par             wikidata_id = fetch_wikidata_entity(term_lower)
\par 
\par         # If Wikidata linking is disabled or Wikidata ID not found, perform fuzzy matching
\par         if not wikidata_id:
\par             fuzzy_match_term = fuzzy_match(term_lower, existing_linked_terms, threshold=fuzzy_threshold)
\par             if fuzzy_match_term:
\par                 # Fetch the corresponding Wikidata ID for the matched term
\par                 matched_entity = linked_entities_collection.find_one(\{"term": fuzzy_match_term.lower()\})
\par                 if matched_entity:
\par                     wikidata_id = matched_entity.get('kb_id')
\par                     logger.debug(f"Fuzzy matched term '\{term_lower\}' to '\{fuzzy_match_term\}' with Wikidata ID '\{wikidata_id\}'.")
\par             else:
\par                 logger.debug(f"No match found for term '\{term_lower\}'.")
\par 
\par         if wikidata_id:
\par             logger.debug(f"Linked term '\{term_lower\}' to Wikidata ID '\{wikidata_id\}'.")
\par 
\par         update = \{
\par             "$inc": \{"frequency": frequency\},
\par             "$addToSet": \{"document_ids": \{"$each": document_ids\}\},
\par             "$set": \{"type": ent_type, "kb_id": wikidata_id\}
\par         \}
\par 
\par         operations.append(
\par             UpdateOne(
\par                 \{"term": term_lower\},
\par                 update,
\par                 upsert=True
\par             )
\par         )
\par 
\par         linked_count += 1
\par         processed_entities += 1
\par 
\par         # Log progress every 1000 entities
