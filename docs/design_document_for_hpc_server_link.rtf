\par import os
\par import logging
\par import json
\par from collections import defaultdict
\par from pymongo import UpdateOne, MongoClient
\par from dotenv import load_dotenv
\par 
\par # Load environment variables from .env file
\par load_dotenv()
\par 
\par # =======================
\par # Logging Configuration
\par # =======================
\par logger = logging.getLogger('UniqueTermsGenerator')
\par logger.setLevel(logging.DEBUG)
\par 
\par if not logger.handlers:
\par     console_handler = logging.StreamHandler()
\par     console_handler.setLevel(logging.INFO)
\par 
\par     file_handler = logging.FileHandler('unique_terms.log', mode='a')
\par     file_handler.setLevel(logging.DEBUG)
\par 
\par     formatter = logging.Formatter('%(asctime)s - %(levelname)s - %(message)s')
\par     console_handler.setFormatter(formatter)
\par     file_handler.setFormatter(formatter)
\par 
\par     logger.addHandler(console_handler)
\par     logger.addHandler(file_handler)
\par 
\par # =======================
\par # Database Functions
\par # =======================
\par 
\par def get_client():
\par     """Initialize and return a new MongoDB client."""
\par     try:
\par         mongo_uri = os.environ.get('MONGO_URI')
\par         if not mongo_uri:
\par             raise ValueError("MONGO_URI environment variable not set")
\par         client = MongoClient(mongo_uri, serverSelectionTimeoutMS=5000)
\par         # Test connection
\par         client.admin.command('ping')
\par         logger.info("Successfully connected to MongoDB.")
\par         return client
\par     except Exception as e:
\par         logger.error(f"Failed to connect to MongoDB: \{e\}")
\par         raise e
\par 
\par def get_db(client):
\par     """Return the database instance."""
\par     return client['railroad_documents']
\par 
\par def get_collections(db):
\par     """Retrieve and return references to the required collections."""
\par     try:
\par         documents_collection = db['documents']
\par         unique_terms_collection = db['unique_terms']
\par         return documents_collection, unique_terms_collection
\par     except Exception as e:
\par         logger.error(f"Error getting collections: \{e\}")
\par         raise e
\par 
\par # =======================
\par # Unique Terms Generation
\par # =======================
\par 
\par def generate_unique_terms(documents_collection, unique_terms_collection, fields_to_process, term_type='word', batch_size=1000):
\par     """
\par     Generate unique terms from specified fields in documents and update the unique_terms collection.
\par     
\par     :param documents_collection: MongoDB collection containing documents
\par     :param unique_terms_collection: MongoDB collection to store unique terms
\par     :param fields_to_process: List of fields to extract terms from
\par     :param term_type: Type of term ('word' or 'phrase')
\par     :param batch_size: Number of documents to process per batch
\par     """
\par     logger.info(f"Starting unique terms generation for term_type: \{term_type\}")
\par     cursor = documents_collection.find(\{\}, \{field: 1 for field in fields_to_process\})
\par     
\par     term_counts = defaultdict(int)
\par     
\par     for doc in cursor.batch_size(batch_size):
\par         for field in fields_to_process:
\par             text = doc.get(field, '')
\par             if text:
\par                 if term_type == 'word':
\par                     words = text.split()
\par                     for word in words:
\par                         clean_word = word.strip().lower()
\par                         if clean_word:
\par                             term_counts[clean_word] += 1
\par                 elif term_type == 'phrase':
\par                     # Example: Extract bigrams as phrases
\par                     words = text.split()
\par                     for i in range(len(words)-1):
\par                         phrase = f"\{words[i].strip().lower()\} \{words[i+1].strip().lower()\}"
\par                         if phrase:
\par                             term_counts[phrase] += 1
\par     
\par     # Prepare bulk operations
\par     operations = []
\par     for term, count in term_counts.items():
\par         operations.append(
\par             UpdateOne(
\par                 \{"term": term, "type": term_type, "field": "ocr_text"\},  # Assuming 'ocr_text' is the field
\par                 \{"$set": \{"term": term, "type": term_type, "field": "ocr_text"\}, "$inc": \{"frequency": count\}\},
\par                 upsert=True
\par             )
\par         )
\par     
\par     if operations:
\par         try:
\par             result = unique_terms_collection.bulk_write(operations, ordered=False)
\par             logger.info(f"Bulk upserted \{result.upserted_count + result.modified_count\} unique terms.")
\par         except Exception as e:
\par             logger.error(f"Error bulk upserting unique terms: \{e\}")
\par             raise e
\par     else:
\par         logger.info("No unique terms to upsert.")
\par 
\par     logger.info("Unique terms generation completed.")
\par 
