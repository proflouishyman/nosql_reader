\par # Main Execution
\par # =======================
\par 
\par if __name__ == "__main__":
\par     try:
\par         client = get_client()
\par         db = get_db(client)
\par         documents_collection, unique_terms_collection = get_collections(db)
\par         
\par         # Define fields to process and term types
\par         fields_to_process = ["ocr_text", "summary"]
\par         term_types = ['word', 'phrase']
\par         
\par         for term_type in term_types:
