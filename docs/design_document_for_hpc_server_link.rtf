\par def get_top_unique_terms(db, field, term_type, query='', limit=1000, skip=0):
\par     """
\par     Retrieve top unique terms based on the field, term type, and optional search query.
\par 
\par     :param db: Database instance
\par     :param field: The field to filter terms by (e.g., 'title', 'description')
\par     :param term_type: The type of term ('word' or 'phrase')
\par     :param query: Optional search query string to filter terms
\par     :param limit: Number of top terms to retrieve
\par     :param skip: Number of records to skip for pagination
\par     :return: List of dictionaries with term and count
\par     """
\par     unique_terms_collection = db['unique_terms']
\par     
\par     try:
\par         # Base MongoDB query
\par         mongo_query = \{"field": field, "type": term_type\}
\par         
\par         # If a search query is provided, add a regex filter for the 'term' field
\par         if query:
\par             # Escape special regex characters to prevent injection attacks
\par             escaped_query = re.escape(query)
\par             # Case-insensitive search for terms containing the query substring
\par             mongo_query['term'] = \{"$regex": f".*\{escaped_query\}.*", "$options": "i"\}
\par         
\par         start_time = time.time()
\par         
\par         # Execute the query with sorting, skipping, and limiting for pagination
\par         cursor = unique_terms_collection.find(
\par             mongo_query,
\par             \{"_id": 0, "term": 1, "frequency": 1\}
\par         ).sort("frequency", pymongo.DESCENDING).skip(skip).limit(limit)
\par         
\par         terms_list = []
\par         for doc in cursor:
\par             key = 'word' if term_type == 'word' else 'phrase'
\par             terms_list.append(\{key: doc['term'], 'count': doc['frequency']\})
\par         
\par         duration = time.time() - start_time
\par         logger.info(f"Retrieved top \{len(terms_list)\} \{term_type\}s in \{duration:.4f\} seconds for field '\{field\}' with query '\{query\}'.")
\par 
\par         return terms_list
\par     except Exception as e:
\par         logger.error(f"Error retrieving unique terms: \{e\}")
\par         return []
